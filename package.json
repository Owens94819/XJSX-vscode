{
  "name": "XJSX-vscode",
  "displayName": "XJSX",
  "description": "Language support for XJSX",
  "version": "0.0.1",
	"author": "Owens Akpede",
	"license": "MIT",
	"publisher": "owens94819",
  "icon": "assets/icon.png",
  "files": [
		"languages/",
		"syntaxes/",
		"assets/"
	],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/owens94819/XJSX-vscode.git"
  },
  "bugs": {
    "url": "https://github.com/owens94819/XJSX-vscode/issues"
  },
  "homepage": "https://github.com/owens94819/XJSX-vscode#readme",
	"qna": false,
  "engines": {
    "vscode": "^1.54.0"
  },
  "categories": [
    "Programming Languages",
		"Formatters",
		"Linters"
  ],
  "keywords": [
		"xjsx",
		"html",
		"javascript"
	],
  "galleryBanner": {
		"color": "#17191E",
		"theme": "dark"
	},
  "contributes": {
    "snippets": [
			{
				"scope": "xjsx",
				"path": "./languages/xjsx.code-snippets"
			}
		],
    "breakpoints": [
			{
				"language": "xjsx"
			}
		],
    "languages": [{
      "id": "xjsx",
      "aliases": ["xjsx"],
      "extensions": [".xjsx",".xjsx.html"],
      "configuration": "./language-configuration.json",
				"icon": {
					"light": "./assets/icon (copy).svg",
					"dark": "./assets/icon (copy).svg"
				}
    }],
    "grammars": [
    {
      "language": "xjsx",
      "scopeName": "source.xjsx",
      "path": "./syntaxes/xjsx.tmLanguage.json",
      "embeddedLanguages": {
        "text.html": "html",
        "source.css": "css",
        "source.json": "json",
        "source.js": "javascript"
      },
      "unbalancedBracketScopes": [
        "keyword.operator.relational",
        "storage.type.function.arrow",
        "keyword.operator.bitwise.shift",
        "meta.brace.angle",
        "punctuation.definition.tag"
      ]
    }]
  }
}
